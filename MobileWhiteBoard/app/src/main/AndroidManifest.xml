<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="project14_1.cookandroid.com.mobilewhiteboard" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar" >
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignupActivity" > </activity>
        <activity android:name=".MainViewActivity" > </activity>
        <activity android:name=".MemoActivity" > </activity>
        <activity android:name=".TeamChoiceActivity" > </activity>
        <activity android:name=".WhiteboardActivity"
            android:screenOrientation="sensorLandscape"> </activity>
        <activity android:name=".TeamTaskActivity" > </activity>
    </application>

</manifest>
